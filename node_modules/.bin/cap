#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules/@capacitor/cli/bin/node_modules:/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules/@capacitor/cli/node_modules:/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules/@capacitor/node_modules:/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules:/private/tmp/primeledger/.pnpm/node_modules:/private/tmp/primeledger/node_modules:/private/tmp/node_modules:/private/node_modules:/node_modules:/tmp/primeledger/.pnpm/node_modules"
else
  export NODE_PATH="/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules/@capacitor/cli/bin/node_modules:/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules/@capacitor/cli/node_modules:/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules/@capacitor/node_modules:/private/tmp/primeledger/.pnpm/@capacitor+cli@8.0.0/node_modules:/private/tmp/primeledger/.pnpm/node_modules:/private/tmp/primeledger/node_modules:/private/tmp/node_modules:/private/node_modules:/node_modules:/tmp/primeledger/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@capacitor/cli/bin/capacitor" "$@"
else
  exec node  "$basedir/../@capacitor/cli/bin/capacitor" "$@"
fi
