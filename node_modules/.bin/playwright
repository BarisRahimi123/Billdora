#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/tmp/primeledger/.pnpm/playwright@1.57.0/node_modules/playwright/node_modules:/private/tmp/primeledger/.pnpm/playwright@1.57.0/node_modules:/private/tmp/primeledger/.pnpm/node_modules:/private/tmp/primeledger/node_modules:/private/tmp/node_modules:/private/node_modules:/node_modules:/tmp/primeledger/.pnpm/node_modules"
else
  export NODE_PATH="/private/tmp/primeledger/.pnpm/playwright@1.57.0/node_modules/playwright/node_modules:/private/tmp/primeledger/.pnpm/playwright@1.57.0/node_modules:/private/tmp/primeledger/.pnpm/node_modules:/private/tmp/primeledger/node_modules:/private/tmp/node_modules:/private/node_modules:/node_modules:/tmp/primeledger/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../playwright/cli.js" "$@"
else
  exec node  "$basedir/../playwright/cli.js" "$@"
fi
